  <DataSets>
    <DataSet Name="dsDataOrden">
      <Query>
        <DataSourceName>DsCommon</DataSourceName>
        <CommandText>select ord.OrderID, ord.OrderDate, ord.CustomerID, cu.ContactName, od.ProductID,  pr.ProductName, od.Quantity, 
pr.UnitPrice, od.Quantity * pr.UnitPrice as lineAmount
from [Order Details] od, Orders ord,  Customers cu, Products pr
where od.ProductID = pr.ProductID
and od.OrderID = ord.OrderID
and ord.CustomerID = cu.CustomerID
order by ord.OrderID;
</CommandText>
      </Query>
